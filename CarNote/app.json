{
  "expo": {
    "name": "CarNote",
    "slug": "carnote",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "carnote",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#0D1B2A",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.carnote.app"
    },
    "android": {
      "package": "com.carnote.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
